<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>PlanEve</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
  <servlet-name>login</servlet-name>
  <servlet-class>com.planeve.LoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>login</servlet-name>
  <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>userLogin</servlet-name>
  <servlet-class>com.planeve.UserLoginController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>userLogin</servlet-name>
  <url-pattern>/userLogin</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>userSignUp</servlet-name>
  <servlet-class>com.planeve.UserSignUpController</servlet-class>
  </servlet>
  <servlet-mapping>
  <servlet-name>userSignUp</servlet-name>
  <url-pattern>/userSignUp</url-pattern>
  </servlet-mapping>
  

</web-app>
